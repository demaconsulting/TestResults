{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "build",
      "command": "dotnet",
      "type": "process",
      "args": [
        "build",
        "${workspaceFolder}/DemaConsulting.TestResults.slnx",
        "/property:GenerateFullPaths=true",
        "/consoleloggerparameters:NoSummary;ForceNoAlign"
      ],
      "problemMatcher": "$msCompile",
      "group": {
        "kind": "build",
        "isDefault": true
      }
    },
    {
      "label": "clean",
      "command": "dotnet",
      "type": "process",
      "args": [
        "clean",
        "${workspaceFolder}/DemaConsulting.TestResults.slnx"
      ],
      "problemMatcher": "$msCompile"
    },
    {
      "label": "restore",
      "command": "dotnet",
      "type": "process",
      "args": [
        "restore",
        "${workspaceFolder}/DemaConsulting.TestResults.slnx"
      ],
      "problemMatcher": "$msCompile"
    },
    {
      "label": "test",
      "command": "dotnet",
      "type": "process",
      "args": [
        "test",
        "${workspaceFolder}/DemaConsulting.TestResults.slnx",
        "/property:GenerateFullPaths=true",
        "/consoleloggerparameters:NoSummary;ForceNoAlign"
      ],
      "problemMatcher": "$msCompile",
      "group": {
        "kind": "test",
        "isDefault": true
      }
    },
    {
      "label": "lint",
      "command": "dotnet",
      "type": "shell",
      "args": [
        "format",
        "--verify-no-changes",
        "${workspaceFolder}/DemaConsulting.TestResults.slnx"
      ],
      "problemMatcher": []
    },
    {
      "label": "format",
      "command": "dotnet",
      "type": "shell",
      "args": [
        "format",
        "${workspaceFolder}/DemaConsulting.TestResults.slnx"
      ],
      "problemMatcher": []
    }
  ]
}
